mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd $(QTDIR)/src/moc && $(MAKE) )

Makefile: MAD.pro  /usr/share/qt3/mkspecs/default/qmake.conf ../../../../../../share/qt3/lib/libqt-mt.prl
	$(QMAKE) -o Makefile MAD.pro
